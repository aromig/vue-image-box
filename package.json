{
  "name": "vue-image-box",
  "version": "1.3.1",
  "description": "Lightweight and simple-ish image gallery component for Vue.js",
  "author": "Adam Romig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/aromig/vue-image-box"
  },
  "main": "dist/vue-image-box.umd.js",
  "module": "dist/vue-image-box.esm.js",
  "unpkg": "dist/vue-image-box.min.js",
  "browser": {
    "./sfc": "src/vue-image-box.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "unit": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.2.0",
    "jest": "^24.9.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.13.1",
    "rollup": "^1.16.6",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^13.2.0",
    "vue": "^2.6.8",
    "vue-jest": "^4.0.1",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
